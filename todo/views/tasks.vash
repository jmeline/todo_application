@html.extend("./template.vash", function(model){

    @html.block("tasks", function(model){
        if (@model.tasks)
        {

            <div class="list-group">
                @model.tasks.forEach(function(item){
              <a href="#" class="list-group-item">@item</a>
                })
            </div>
        }
        else
        {
            <h4>Nothing TODO, add a TODO! </h4>
        }

    })
})